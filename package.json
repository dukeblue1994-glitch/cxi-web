{
  "name": "cxi-demo-v15b",
  "version": "1.0.0",
  "description": "Customer Experience Intelligence (CXI) feedback collection and analysis system",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && netlify dev --port 8888 --dir dist --functions netlify/functions",
    "build": "rm -rf dist && mkdir -p dist && cp index.html styles.css -t dist && cp -R js dist/js",
    "preview": "npx http-server dist -p 4173",
    "test": "node tests/scores.test.js || true",
    "test:reliability": "node test/test-reliability.js",
    "test:reliability:dev": "npx netlify dev --port 8888 --dir dist --functions netlify/functions & sleep 3 && npm run test:reliability; kill $! || true",
    "deploy": "netlify deploy --prod",
    "deploy:preview": "netlify deploy"
  },
  "keywords": [
    "customer-experience",
    "feedback",
    "analytics",
    "netlify",
    "serverless",
    "javascript"
  ],
  "author": "Nick Anderson",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@netlify/blobs": "^10.0.11",
    "@netlify/functions": "^2.6.0",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@netlify/edge-functions": "^2.18.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "netlify-cli": "^23.8.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dukeblue1994-glitch/cxi-web.git"
  },
  "bugs": {
    "url": "https://github.com/dukeblue1994-glitch/cxi-web/issues"
  },
  "homepage": "https://github.com/dukeblue1994-glitch/cxi-web#readme"
}
