{
  "name": "cxi-demo-v15b",
  "version": "1.0.0",
  "description": "Customer Experience Intelligence (CXI) feedback collection and analysis system",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && netlify dev --dir dist --functions netlify/functions --port 8888 --offline --no-open",
    "build": "rm -rf dist && mkdir -p dist && cp -R js dist/js && cp -R css dist/css && cp index.html dist/index.html",
    "test": "npm run build && node --test \"test/**/*.test.js\"",
    "test:unit": "npm run build && node --test \"test/unit/**/*.test.js\"",
    "test:e2e": "npm run build && node --test \"test/e2e/**/*.test.js\"",
    "test:reliability": "node --test \"test/reliability/**/*.test.js\"",
    "lint": "eslint netlify/functions/**/*.js",
    "lint:fix": "eslint netlify/functions/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,json,md,css,html}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,css,html}\"",
    "clean": "rm -rf .netlify dist"
  },
  "keywords": [
    "customer-experience",
    "feedback",
    "analytics",
    "netlify",
    "serverless",
    "javascript"
  ],
  "author": "Nick Anderson",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@netlify/blobs": "^10.0.11",
    "@netlify/functions": "^2.6.0",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@netlify/edge-functions": "^2.18.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "jsdom": "^26.0.0",
    "netlify-cli": "^23.8.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dukeblue1994-glitch/cxi-web.git"
  },
  "bugs": {
    "url": "https://github.com/dukeblue1994-glitch/cxi-web/issues"
  },
  "homepage": "https://github.com/dukeblue1994-glitch/cxi-web#readme"
}
